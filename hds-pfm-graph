		cap => [
			'CTL',
			'Write Pending Rate(%)',
		],
	}, {
		subsys => 'Cache',
		key => [ 'CTL' ],
		re => qr/^CTL\s+Clean Queue Usage Rate/,
		cap => [
			'CTL',
			'Clean Queue Usage Rate(%)',
			'Middle Queue Usage Rate(%)',
			'Physical Queue Usage Rate(%)',
			'Total Queue Usage Rate(%)',
		],
	}, {
		subsys => 'Cache',
		key => [ 'CTL', 'Partition' ],
		re => qr/^CTL\s+Partition\s+Write Pending Rate/,
		cap => [
